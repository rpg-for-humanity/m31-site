{
  "schema": "rpg4h.dialogueSet.v1",
  "version": "0.1.0",
  
  "metadata": {
    "id": "dialogue.cafe.set001",
    "title": "Coffee Outpost - Beginner Set",
    "description": "Basic coffee ordering scenarios for first-time learners",
    "author": {
      "id": "creator_clair",
      "name": "Clair Hur",
      "verified": true
    },
    "createdAt": "2025-01-20T10:00:00Z",
    "updatedAt": "2025-01-20T10:00:00Z",
    "tags": ["coffee", "ordering", "beginner", "cafe"],
    "difficulty": "beginner",
    "estimatedMinutes": 15,
    "parentPack": "pack.m31.cafe.coffeepost",
    "income": {
      "priceCoins": 0,
      "revenueShare": 0.7
    }
  },

  "supportedLanguages": ["en-US", "ko-KR", "fr-FR"],
  "defaultLanguage": "en-US",

  "rounds": [
    {
      "id": "r1",
      "mode": "listen_select",
      "itemCount": 1,
      "dialogues": {
        "en-US": {
          "npcOrderText": "Hi, a caramel macchiato, medium please.",
          "npcOrderAudio": "/Audio/en/set001-r1-order.mp3"
        },
        "ko-KR": {
          "npcOrderText": "안녕하세요, 카라멜 마끼아또 미디움 사이즈로 주세요.",
          "npcOrderAudio": "/Audio/ko/set001-r1-order.mp3"
        },
        "fr-FR": {
          "npcOrderText": "Bonjour, un macchiato caramel, taille moyenne s'il vous plaît.",
          "npcOrderAudio": "/Audio/fr/set001-r1-order.mp3"
        }
      },
      "correctOrder": [
        { "type": "Macchiato", "size": "Medium", "syrup": "Caramel" }
      ],
      "hints": {
        "en-US": ["Listen for the syrup flavor", "What size did they say?"],
        "ko-KR": ["시럽 맛을 들어보세요", "사이즈가 뭐라고 했나요?"],
        "fr-FR": ["Écoutez la saveur du sirop", "Quelle taille ont-ils dit?"]
      }
    },
    {
      "id": "r2",
      "mode": "listen_select",
      "itemCount": 1,
      "dialogues": {
        "en-US": {
          "npcOrderText": "Hi, I'd like a large flat white with hazelnut syrup please.",
          "npcOrderAudio": "/Audio/en/set001-r2-order.mp3"
        },
        "ko-KR": {
          "npcOrderText": "안녕하세요, 라지 사이즈 플랫 화이트에 헤이즐넛 시럽 넣어주세요.",
          "npcOrderAudio": "/Audio/ko/set001-r2-order.mp3"
        },
        "fr-FR": {
          "npcOrderText": "Bonjour, je voudrais un grand flat white avec du sirop de noisette s'il vous plaît.",
          "npcOrderAudio": "/Audio/fr/set001-r2-order.mp3"
        }
      },
      "correctOrder": [
        { "type": "Flat White", "size": "Large", "syrup": "Hazelnut" }
      ]
    },
    {
      "id": "r3",
      "mode": "listen_select",
      "itemCount": 3,
      "dialogues": {
        "en-US": {
          "npcOrderText": "Can I get a large Americano, and two small lattes — one with oat milk and one with whole milk?",
          "npcOrderAudio": "/Audio/en/set001-r3-order.mp3"
        },
        "ko-KR": {
          "npcOrderText": "라지 아메리카노 하나랑, 스몰 라떼 두 개 주세요. 하나는 오트밀크, 하나는 일반 우유로요.",
          "npcOrderAudio": "/Audio/ko/set001-r3-order.mp3"
        },
        "fr-FR": {
          "npcOrderText": "Je voudrais un grand Americano et deux petits lattés — un avec du lait d'avoine et un avec du lait entier.",
          "npcOrderAudio": "/Audio/fr/set001-r3-order.mp3"
        }
      },
      "correctOrder": [
        { "type": "Americano", "size": "Large" },
        { "type": "Latte", "size": "Small", "milk": "Oat" },
        { "type": "Latte", "size": "Small", "milk": "Whole" }
      ]
    },
    {
      "id": "r4",
      "mode": "chat_fill",
      "dialogues": {
        "en-US": {
          "npcOpening": "Hi there! What can I get started for you today?",
          "npcOpeningAudio": "/Audio/en/set001-r4-greeting.mp3",
          "followUps": {
            "missingType": { "text": "What kind of coffee would you like?", "audio": "/Audio/en/ask-type.mp3" },
            "missingSize": { "text": "What size?", "audio": "/Audio/en/ask-size.mp3" },
            "missingTemp": { "text": "Would you like that hot or iced?", "audio": "/Audio/en/ask-temp.mp3" },
            "missingMilk": { "text": "Any milk preference?", "audio": "/Audio/en/ask-milk.mp3" }
          }
        },
        "ko-KR": {
          "npcOpening": "안녕하세요! 주문 도와드릴까요?",
          "npcOpeningAudio": "/Audio/ko/set001-r4-greeting.mp3",
          "followUps": {
            "missingType": { "text": "어떤 커피 드릴까요?", "audio": "/Audio/ko/ask-type.mp3" },
            "missingSize": { "text": "사이즈는요?", "audio": "/Audio/ko/ask-size.mp3" },
            "missingTemp": { "text": "따뜻하게 드릴까요, 아이스로 드릴까요?", "audio": "/Audio/ko/ask-temp.mp3" },
            "missingMilk": { "text": "우유는 어떤 걸로 드릴까요?", "audio": "/Audio/ko/ask-milk.mp3" }
          }
        },
        "fr-FR": {
          "npcOpening": "Bonjour! Qu'est-ce que je vous sers?",
          "npcOpeningAudio": "/Audio/fr/set001-r4-greeting.mp3",
          "followUps": {
            "missingType": { "text": "Quel type de café voulez-vous?", "audio": "/Audio/fr/ask-type.mp3" },
            "missingSize": { "text": "Quelle taille?", "audio": "/Audio/fr/ask-size.mp3" },
            "missingTemp": { "text": "Chaud ou glacé?", "audio": "/Audio/fr/ask-temp.mp3" },
            "missingMilk": { "text": "Quel lait préférez-vous?", "audio": "/Audio/fr/ask-milk.mp3" }
          }
        }
      },
      "requirements": ["type", "size", "temp", "milk"]
    },
    {
      "id": "r5",
      "mode": "speech_fill",
      "dialogues": {
        "en-US": {
          "npcOpening": "Welcome! What can I get for you today?",
          "npcOpeningAudio": "/Audio/en/set001-r5-greeting.mp3",
          "confirmOrder": "Got it! One {order}. That'll be ready at the pickup counter.",
          "confirmAudio": "/Audio/en/confirm-order.mp3"
        },
        "ko-KR": {
          "npcOpening": "어서오세요! 뭘 드릴까요?",
          "npcOpeningAudio": "/Audio/ko/set001-r5-greeting.mp3",
          "confirmOrder": "네! {order} 주문 받았습니다. 픽업 카운터에서 받아가세요.",
          "confirmAudio": "/Audio/ko/confirm-order.mp3"
        },
        "fr-FR": {
          "npcOpening": "Bienvenue! Qu'est-ce que je vous sers?",
          "npcOpeningAudio": "/Audio/fr/set001-r5-greeting.mp3",
          "confirmOrder": "C'est noté! Un {order}. Ce sera prêt au comptoir.",
          "confirmAudio": "/Audio/fr/confirm-order.mp3"
        }
      },
      "requirements": ["type", "size", "temp", "milk", "syrup"],
      "speechConfig": {
        "en-US": { "lang": "en-US", "timeoutMs": 8000 },
        "ko-KR": { "lang": "ko-KR", "timeoutMs": 10000 },
        "fr-FR": { "lang": "fr-FR", "timeoutMs": 9000 }
      }
    }
  ]
}
